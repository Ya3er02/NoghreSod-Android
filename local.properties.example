# Copy this file to local.properties and fill in your values

# Development API
dev.api.url=https://dev-api.noghresod.com/api/v1/
dev.api.timeout=30

# Staging API
staging.api.url=https://staging-api.noghresod.com/api/v1/
staging.api.timeout=30

# Production API
prod.api.url=https://api.noghresod.com/api/v1/
prod.api.timeout=30

# Keystore for signing
keystore.file=./keystore/noghresod.jks
keystore.password=YOUR_KEYSTORE_PASSWORD
keystore.alias=noghresod
keystore.key.password=YOUR_KEY_PASSWORD

# Firebase
firebase.enabled=true
firebase.project.id=YOUR_PROJECT_ID

# Analytics
analytics.enabled=true
